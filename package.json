    "css-watch": "npm run css-build -- --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",

      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "bulma": "^0.9.4",
    "gh-pages": "^4.0.0",
    "node-sass": "^7.0.1"
